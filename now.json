{
  "version": 2,
  "name": "PassionInActionWebsite",
  "routes": [
    {
      "src": "^/sw.js$",
      "dest": "/static/sw.js",
      "headers": {
        "Service-Worker-Allowed": "/"
      }
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    },
    {
      "src": "/api/*",
      "use": "@now/node"
    }
  ]
}
