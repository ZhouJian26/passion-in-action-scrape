{
  "version": 2,
  "routes": [{
    "src": "^/sw.js$",
    "headers": {
      "Service-Worker-Allowed": "/"
    }
  }],
  "builds": [{
    "src": "package.json",
    "use": "@now/next"
  }],
  "env": {
    "AWS_S3_REGION": "@aws_s3_region",
    "AWS_S3_BUCKET_NAME": "@aws_s3_bucket_region",
    "AWS_S3_KEY_FILE_NAME": "@aws_s3_key_file_name",
    "AWS_ACCESS_KEY_ID": "@aws_access_key_id",
    "AWS_SECRET_ACCESS_KEY": "@aws_secret_access_key"

  }
}